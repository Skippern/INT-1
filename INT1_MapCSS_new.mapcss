node["seamark:landmark:function"=mosque].landmark::landmark {
	icon-image:		"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/E/E17.svg";
}
node["seamark:landmark:category"=minaret].landmark::landmark {
	icon-image:		"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/E/E17.svg";
