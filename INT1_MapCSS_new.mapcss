}

node["seamark:type"=marine_farm]::marine_farm {
	icon-image:		"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/K/K48.1_no_red.svg";
	icon-width:		28;
}
node["seamark:type"=wreck]::obstruction {
	set				wreck;
	icon-image:		"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/K/K29.svg";
	icon-width:		28;
	text-color:		black;
	font-size:		12;
}
node["seamark:wreck:category"=non-dangerous]::obstruction {
	icon-image:		"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/K/K29.svg";
}
node["seamark:wreck:category"=dangerous]::obstruction {
