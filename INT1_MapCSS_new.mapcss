/* L: Offshore Installations */
way["seamark:type"=caable_submarine]::cable {
	repeat-image:			"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/L/L31.svg";
	repeat-image-width:		32;
/*	repeat-image-spacing:	2; */
}
way["seamark:type"=pipeline_submarine]::pipeline {
	repeat-image:			"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/L/L40.svg";
	repeat-image-width:		32;
	repeat-image-spacing:	2;
}

/* M: Tracks, Routes */
way["seamark:type"=recommended_track]::recommended_track {
	color:			black;
	fill-color:		black;
	width:			3;
	opacity:		1.0;
}
node["seamark:type"=calling-in_point]::RDOCAL {
	icon-image:		eval(concat("https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/M/M40.", cond(tag("seamark:calling-in_point:traffic_flow") == "one-way", "1", "2") , ".svg"));
	icon-height:	24;
	text:			"seamark:name";
	text-color:		darkmagenta;
	font-size:		18;
	text-anchor-vertical:	above;
	text-anchor-horizontal:	center;
}
way["seamark:type"=ferry_route]::ferry_route {
	color:			darkmagenta;
	fill-color:		darkmagenta;
	width:			2;
	opacity:		1.0;
	dashes:			5,8;
}

/* N: Areas, Limites */
way[boundary],
area[boundary],
relation[boundary] > way::border {
	set				border;
}
*[boundary=administrative][!maritime].border::border,
*[boundary=administrative][maritime?!].border::border,
relation[boundary=administrative] > way[!maritime].border::border,
relation[boundary=administrative] > way[maritime?!].border::border {
	repeat-image:			"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/N/N40.svg";
	repeat-image-width:		eval(500 / any(tag(admin_level), "2"));
	repeat-image-spacing:	3;
	opacity:				0.05;
}
*[boundary=maritime][border_type=territorial].border::border,
*[boundary=administrative][maritime?].border::border,
relation[boundary=administrative] > way[maritime?].border::border,
relation[boundary=administrative] > way[border_type=territorial].border::border {
	repeat-image:			"https://raw.githubusercontent.com/Skippern/INT-1/master/SVG/N/N41.svg";
	repeat-image-width:		eval(500 / any(tag(admin_level), "2" ));
	repeat-image-spacing:	3;
	opacity:				0.05;
}
